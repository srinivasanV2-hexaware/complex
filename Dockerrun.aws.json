{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "sriniv23/multi-client",
      "hostname": "client",
      "essential": false,
      "memory": 128
    },
    {
      "name": "server",
      "image": "sriniv23/multi-server",
      "hostname": "api",
      "essential": false,
      "memory": 128
    },
    {
      "name": "worker",
      "image": "sriniv23/multi-worker",
      "hostname": "worker",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "sriniv23/multi-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [{
          "hostPort": 80,
          "containerPort": 80
      }],
      "links": ["client","server"],
      "memory": 128
    }
  ]
}